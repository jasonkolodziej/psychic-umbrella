		"prosemirror-schema-basic": "^1.2.3",
		"prosemirror-schema-list": "^1.4.1",
		"prosemirror-state": "^1.4.3",
		"prosemirror-view": "^1.34.2",
		"rehype": "^13.0.1",
		"slugify": "^1.6.6",
		"svelte-json-tree": "^2.2.0",
		"tailwind-merge": "^2.5.2",
		"tiptap-markdown": "^0.8.10"
	}
}
