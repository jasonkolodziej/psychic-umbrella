{
	"name": "psychic-umbrella",
	"version": "0.0.2",
	"private": true,
	"packageManager": "pnpm@9.9.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "pnpm run build && wrangler pages dev",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "pnpm run build && wrangler pages deploy",
		"deploy:dryrun": "pnpm run build && wrangler pages deploy --dry-run",
		"cf-typegen": "wrangler types && mv worker-configuration.d.ts src/",
		"build:local": "CF_PAGES=1 vite build",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"roll:workers": "rollup -c rollup.workers.js",
		"use:md:carta": "pnpm add carta-md @cartamd/plugin-attachment @cartamd/plugin-code @cartamd/plugin-component @cartamd/plugin-emoji @cartamd/plugin-slash",
		"use:md:marked": "pnpm i -D @magidoc/plugin-svelte-marked marked @types/marked"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240909.0",
		"@magidoc/plugin-svelte-marked": "^6.0.1",
		"@playwright/test": "^1.47.1",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@sveltejs/adapter-auto": "^3.2.5",
		"@sveltejs/adapter-cloudflare": "^4.7.2",
		"@sveltejs/kit": "^2.5.28",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^9.6.1",
		"@types/hast": "^3.0.4",
		"autoprefixer": "^10.4.20",
		"dayjs": "^1.11.13",
		"eslint": "^9.10.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.44.0",
		"flowbite-svelte-blocks": "^1.1.3",
		"globals": "^15.9.0",
		"marked": "^14.1.2",
		"mdsvex": "^0.12.3",
		"postcss": "^8.4.47",
		"postcss-apply": "^0.12.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"rollup": "^4.21.3",
		"rollup-plugin-svelte": "^7.2.2",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"svelte-lib-helpers": "^0.4.8",
		"svelte-meta-tags": "^3.1.4",
		"tailwindcss": "^3.4.12",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.6.0",
		"vite": "^5.4.6",
		"vitest": "^2.1.1",
		"wrangler": "^3.78.3"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "^0.34.2",
		"@auth/d1-adapter": "^1.5.0",
		"@auth/sveltekit": "^1.5.0",
		"@cloudflare/pages-plugin-cloudflare-access": "^1.0.5",
